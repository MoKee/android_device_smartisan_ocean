# Bluetooth
/(vendor|system/vendor)/bin/bt-mac-generator	u:object_r:bt_mac_generator_exec:s0

# Firmware
/firmware					u:object_r:firmware_file:s0
/bt_firmware					u:object_r:bt_firmware_file:s0

# Persist
/persist(/.*)?					u:object_r:mnt_vendor_file:s0

# Light
/(vendor|system/vendor)/bin/hw/android\.hardware\.light@2\.0-service.ocean	u:object_r:hal_light_default_exec:s0

# Logic volume
/(vendor|system/vendor)/bin/dm-tool		u:object_r:logic_volume_exec:s0
/(vendor|system/vendor)/bin/logic_volume	u:object_r:logic_volume_exec:s0

# Block device
/dev/block/platform/soc/7c4000.sdhci/by-name/ramdump	u:object_r:ramdump_block_device:s0
/dev/block/platform/soc/1d84000.ufshc/su[a-z]		u:object_r:sec_block_device:s0
